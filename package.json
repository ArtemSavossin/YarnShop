  "bugs": {
    "url": "https://github.com/ArtemSavossin/YarnShop/issues"
  },
  "homepage": "https://github.com/ArtemSavossin/YarnShop#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.12",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.17",
    "path": "^0.12.7",
    "react-html-email": "^3.0.0",
    "sendmail": "^1.6.1"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
